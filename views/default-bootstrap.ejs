<!DOCTYPE html>
<html lang="en">
  <head>
	<% include partials/head.ejs %>
  </head>
  <body class="<%= classname %>">
	
	<% if (typeof(titleofthewebsite) !== 'undefined') { %> <!-- in case app.locals.titleofthewebsite not set-->
    <h1><%= titleofthewebsite %></h1><br/>
	<% } %>
	
	
	<p>Title: <%= title %>  </p> <!-- // Variable passed to it -->
	
	<h3>Array Output:</h3>
	
	<% if (typeof(users) !== 'undefined') { %> <!-- if "users" array !exist -->
	<ul>
		<% for(var i=0; i<users.length; i++){ %>
		<li> <%= users[i] %> </li>
		<% } %>
	</ul>
	<% } %>
	
	<h3>Object</h3>
	<% if (typeof(social) !== 'undefined') { %> <!-- if "social" object !exist -->
	<ul>
		<% for(var i=0; i<social.length; i++){ %>
		<li> <%= social[i].name %> </li>
		<% } %>
	</ul>
	<% } %>
	
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
  </body>
</html>